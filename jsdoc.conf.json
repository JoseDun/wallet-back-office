{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown", "better-docs/component"],
	"templates": {
		"better-docs": {
			"name": "Kilimanjaro"
		}
	},
	"opts": {
		"destination": "jsdocs",
		"recurse": true,
		"readme": "README.md"
	}
}
